PORT=3002
NODE_ENV=development
LOG_LEVEL=info
REDIS_URL=redis://localhost:6379

# CORS configuration
# Comma-separated list of allowed origins for CORS
# Use '*' to allow all origins (not recommended for production)
# Example: http://localhost:3000,https://cloudicons.example.com
ALLOWED_ORIGINS=http://localhost:3000,https://cloudicons.example.com

# Cache configuration
CACHE_ENABLED=true
CACHE_TTL=3600
ICON_CACHE_TTL=86400
SVG_CACHE_TTL=604800
MEMORY_CACHE_SIZE=1000

# API key configuration
# Format: KEY1:NAME1:STANDARD_LIMIT1:SVG_LIMIT1:METADATA_LIMIT1,KEY2:NAME2:...
# Example: api_key_1:Developer:500:1000:200,api_key_2:Premium:1000:2000:500
API_KEYS=example_key:Example:300:800:150
